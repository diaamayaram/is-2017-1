    def getSuccessors(self, state):
        
        def doleft(state, row):
            state_new = state # tal vez no necesaria
            d = state_new[row][0]
            for i in range(7):
                state_new[row][i] = state_new[row][i+1] 
            state_new[row][7] = d 
            return state_new

        def doright(state, row):
            state_new = state # tal vez no necesaria
            d = state_new[row][6]
            limit=range(7)
            limit.reverse()
            for i in limit:       
                state_new[row][i] = state_new[row][i-1]
            state_new[row][7] = d
            return state_new
        
        def doturn(state, column):
            state_new = state # tal vez no necesaria
            a = []
            for i in range(4):
                a.append(state_new[i][column : (column + 4) % 8])
                a[i].reverse()
            a.reverse() 
            for i in range(4):
                for j in range(4):
                    state_new[i][(column + j) % 8] = a[i][j]
            return state_new
        
        lst_successors = []    
                
        for i in range(4):
            state_new = state
            for j in range(7):
                state_new = doright(state_new, i)
